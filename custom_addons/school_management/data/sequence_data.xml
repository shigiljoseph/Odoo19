<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
<!--    Generate sequence number    -->
        <record id="ir_sequence_school_management" model="ir.sequence">
           <field name="name">Serial Numbers</field>
           <field name="code">registration.model</field>
           <field name="prefix">SM</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>


<!--        Generate admission number    -->
        <record id="ir_admission_school_management" model="ir.sequence">
           <field name="name">Admission Numbers</field>
           <field name="code">admission.model</field>
           <field name="prefix">SA2025</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

   </data>
</odoo>